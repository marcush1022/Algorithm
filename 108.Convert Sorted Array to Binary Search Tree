
/*struct TreeNode {
     int val;
     TreeNode *left;
     TreeNode *right;
     TreeNode(int x) : val(x), left(NULL), right(NULL) {}
 };*/
 
class Solution {
public:
	TreeNode* getTree(int start, int end, vector<int>& nums)
	{
		if(start>=end)
			return NULL;
		int mid=(start+end)/2;
		
		TreeNode* cur=new TreeNode(nums[mid]);
		cur->left=getTree(start, mid, nums);
		cur->right=getTree(mid+1, end, nums);
		
		return cur;
		
	}
    TreeNode* sortedArrayToBST(vector<int>& nums) {
        return getTree(0, nums.size(), nums);
    }
    
};

