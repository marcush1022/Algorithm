/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode(int x) : val(x), left(NULL), right(NULL) {}
 * };
 */
class Solution {
public:
    vector<vector<int>> levelOrder(TreeNode* root) 
	  {
	    vector<vector<int>> ret;
	    if(root==NULL)
	        return ret ;
	        
              int level=0;
	      int count=1;
	      vector<int> path;
	  
	      queue<TreeNode*> q;
	      q.push(root);
	   
	      while(!q.empty())
	      {
			      path.clear();
			      level=0;
		
			      for(int i=0;i<count;i++)
			      {
				        root=q.front();
				        q.pop();
				        path.push_back(root->val);
			
				        if(root->left!=NULL)
				        {
					         q.push(root->left);
					         level++;
				         }
				         if(root->right!=NULL)
				        {
					          q.push(root->right);
					          level++;
				        }
			        }
			        count=level;
			        ret.push_back(path);
	          }
	          return ret;
	   
        }
};
